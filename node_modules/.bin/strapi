#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/engine/project/node_modules/.pnpm/@strapi+strapi@5.29.0_@babel+runtime@7.28.4_@codemirror+autocomplete@6.19.1_@codemirror_30278d8f83a6a1077f07a404c69b3656/node_modules/@strapi/strapi/bin/node_modules:/home/engine/project/node_modules/.pnpm/@strapi+strapi@5.29.0_@babel+runtime@7.28.4_@codemirror+autocomplete@6.19.1_@codemirror_30278d8f83a6a1077f07a404c69b3656/node_modules/@strapi/strapi/node_modules:/home/engine/project/node_modules/.pnpm/@strapi+strapi@5.29.0_@babel+runtime@7.28.4_@codemirror+autocomplete@6.19.1_@codemirror_30278d8f83a6a1077f07a404c69b3656/node_modules/@strapi/node_modules:/home/engine/project/node_modules/.pnpm/@strapi+strapi@5.29.0_@babel+runtime@7.28.4_@codemirror+autocomplete@6.19.1_@codemirror_30278d8f83a6a1077f07a404c69b3656/node_modules:/home/engine/project/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/engine/project/node_modules/.pnpm/@strapi+strapi@5.29.0_@babel+runtime@7.28.4_@codemirror+autocomplete@6.19.1_@codemirror_30278d8f83a6a1077f07a404c69b3656/node_modules/@strapi/strapi/bin/node_modules:/home/engine/project/node_modules/.pnpm/@strapi+strapi@5.29.0_@babel+runtime@7.28.4_@codemirror+autocomplete@6.19.1_@codemirror_30278d8f83a6a1077f07a404c69b3656/node_modules/@strapi/strapi/node_modules:/home/engine/project/node_modules/.pnpm/@strapi+strapi@5.29.0_@babel+runtime@7.28.4_@codemirror+autocomplete@6.19.1_@codemirror_30278d8f83a6a1077f07a404c69b3656/node_modules/@strapi/node_modules:/home/engine/project/node_modules/.pnpm/@strapi+strapi@5.29.0_@babel+runtime@7.28.4_@codemirror+autocomplete@6.19.1_@codemirror_30278d8f83a6a1077f07a404c69b3656/node_modules:/home/engine/project/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@strapi/strapi/bin/strapi.js" "$@"
else
  exec node  "$basedir/../@strapi/strapi/bin/strapi.js" "$@"
fi
