#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/engine/project/node_modules/.pnpm/@strapi+strapi@5.29.0_@babel+runtime@7.28.4_@codemirror+autocomplete@6.19.1_@codemirror+langu_hlypkgeuqacjvqzyfnktrvr44q/node_modules/@strapi/strapi/bin/node_modules:/home/engine/project/node_modules/.pnpm/@strapi+strapi@5.29.0_@babel+runtime@7.28.4_@codemirror+autocomplete@6.19.1_@codemirror+langu_hlypkgeuqacjvqzyfnktrvr44q/node_modules/@strapi/strapi/node_modules:/home/engine/project/node_modules/.pnpm/@strapi+strapi@5.29.0_@babel+runtime@7.28.4_@codemirror+autocomplete@6.19.1_@codemirror+langu_hlypkgeuqacjvqzyfnktrvr44q/node_modules/@strapi/node_modules:/home/engine/project/node_modules/.pnpm/@strapi+strapi@5.29.0_@babel+runtime@7.28.4_@codemirror+autocomplete@6.19.1_@codemirror+langu_hlypkgeuqacjvqzyfnktrvr44q/node_modules:/home/engine/project/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/engine/project/node_modules/.pnpm/@strapi+strapi@5.29.0_@babel+runtime@7.28.4_@codemirror+autocomplete@6.19.1_@codemirror+langu_hlypkgeuqacjvqzyfnktrvr44q/node_modules/@strapi/strapi/bin/node_modules:/home/engine/project/node_modules/.pnpm/@strapi+strapi@5.29.0_@babel+runtime@7.28.4_@codemirror+autocomplete@6.19.1_@codemirror+langu_hlypkgeuqacjvqzyfnktrvr44q/node_modules/@strapi/strapi/node_modules:/home/engine/project/node_modules/.pnpm/@strapi+strapi@5.29.0_@babel+runtime@7.28.4_@codemirror+autocomplete@6.19.1_@codemirror+langu_hlypkgeuqacjvqzyfnktrvr44q/node_modules/@strapi/node_modules:/home/engine/project/node_modules/.pnpm/@strapi+strapi@5.29.0_@babel+runtime@7.28.4_@codemirror+autocomplete@6.19.1_@codemirror+langu_hlypkgeuqacjvqzyfnktrvr44q/node_modules:/home/engine/project/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.pnpm/@strapi+strapi@5.29.0_@babel+runtime@7.28.4_@codemirror+autocomplete@6.19.1_@codemirror+langu_hlypkgeuqacjvqzyfnktrvr44q/node_modules/@strapi/strapi/bin/strapi.js" "$@"
else
  exec node  "$basedir/../.pnpm/@strapi+strapi@5.29.0_@babel+runtime@7.28.4_@codemirror+autocomplete@6.19.1_@codemirror+langu_hlypkgeuqacjvqzyfnktrvr44q/node_modules/@strapi/strapi/bin/strapi.js" "$@"
fi
