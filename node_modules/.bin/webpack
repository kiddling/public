#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/engine/project/node_modules/.pnpm/webpack@5.102.1_esbuild@0.25.11/node_modules/webpack/bin/node_modules:/home/engine/project/node_modules/.pnpm/webpack@5.102.1_esbuild@0.25.11/node_modules/webpack/node_modules:/home/engine/project/node_modules/.pnpm/webpack@5.102.1_esbuild@0.25.11/node_modules:/home/engine/project/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/engine/project/node_modules/.pnpm/webpack@5.102.1_esbuild@0.25.11/node_modules/webpack/bin/node_modules:/home/engine/project/node_modules/.pnpm/webpack@5.102.1_esbuild@0.25.11/node_modules/webpack/node_modules:/home/engine/project/node_modules/.pnpm/webpack@5.102.1_esbuild@0.25.11/node_modules:/home/engine/project/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.pnpm/webpack@5.102.1_esbuild@0.25.11/node_modules/webpack/bin/webpack.js" "$@"
else
  exec node  "$basedir/../.pnpm/webpack@5.102.1_esbuild@0.25.11/node_modules/webpack/bin/webpack.js" "$@"
fi
