{
  "kind": "collectionType",
  "collectionName": "design_log_templates",
  "info": {
    "singularName": "design-log-template",
    "pluralName": "design-log-templates",
    "displayName": "Design Log Template",
    "description": "Templates for design log entries with guidance content"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "slug": {
      "type": "uid",
      "targetField": "title",
      "required": true
    },
    "title": {
      "type": "string",
      "required": true,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "description": {
      "type": "text",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "sections": {
      "type": "json",
      "required": true,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "defaultTimelineMilestones": {
      "type": "json",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "exampleDecisions": {
      "type": "json",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "helpTips": {
      "type": "richtext",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "order": {
      "type": "integer",
      "default": 0
    },
    "version": {
      "type": "string",
      "default": "1.0.0"
    },
    "courseCode": {
      "type": "string",
      "required": true
    }
  }
}
